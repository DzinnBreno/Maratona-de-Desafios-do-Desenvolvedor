programa {
  funcao inicio() {
    real mediaFinal, frequencia

    escreva("Digite sua Média Final: ")
    leia(mediaFinal)

    escreva("Digite seu Percentual de Frequência: ")
    leia(frequencia)

    se(mediaFinal>=7.0 e frequencia>=75)
    escreva("Aprovado")
    senao 
    escreva("Reprovado")

  }
}
