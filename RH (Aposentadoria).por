programa {
  funcao inicio() {
    inteiro idade, tempoContribuicao

    escreva("Digite sua idade: ")
    leia(idade)
    escreva("Digite se tempo de contribuição: ")
    leia(tempoContribuicao)

    se(idade > 65 ou tempoContribuicao > 30)
    escreva("Pode se aposentar")
    senao
    escreva("Não pode se aposentar")



  }
}
